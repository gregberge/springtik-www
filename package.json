{
  "name": "springtik-www",
  "version": "0.0.0",
  "description": "Springtik website.",
  "main": "index.js",
  "engines": {
    "node": "6.2.2"
  },
  "scripts": {
    "dev": "npm run dev:server & npm run dev:webpack:browser & npm run dev:webpack:server",
    "dev:server": "nf -j Procfile.dev start",
    "dev:webpack:browser": "webpack-dev-server --no-info --inline --hot --config config/webpack/browser/development.babel.js",
    "dev:webpack:server": "webpack --watch --progress --config config/webpack/server/development.babel.js",
    "postinstall": "npm run build && npm run db:migrate",
    "db:migrate": "knex migrate:latest",
    "db:seed": "knex seed:run",
    "build:lib": "rm -rf lib/ && cp -R src/ lib/ && babel -d lib src --ignore src/modules/babelHelpers.js",
    "build:webpack:server": "webpack --config ./config/webpack/server/production.babel.js",
    "build:webpack:browser": "webpack --config ./config/webpack/browser/production.babel.js",
    "build": "npm run build:lib && npm run build:webpack:server && npm run build:webpack:browser"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Springtik/springtik-www.git"
  },
  "author": "Greg Berg√© <berge.greg@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Springtik/springtik-www/issues"
  },
  "homepage": "http://www.springtik.fr/",
  "dependencies": {
    "amqplib": "^0.4.1",
    "autofill-event": "0.0.1",
    "autoprefixer": "^6.5.0",
    "babel-cli": "^6.16.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-module-resolver": "^2.2.0",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-syntax-trailing-function-commas": "^6.13.0",
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-plugin-transform-regenerator": "^6.16.1",
    "babel-polyfill": "^6.7.4",
    "babel-preset-doctolib": "^2.1.0",
    "bcryptjs": "^2.3.0",
    "body-parser": "^1.15.0",
    "case-sensitive-paths-webpack-plugin": "^1.1.4",
    "classnames": "^2.2.3",
    "compression": "^1.6.1",
    "connect-redis": "^3.0.2",
    "convict": "^1.5.0",
