{
  "presets": ["doctolib"],
  "plugins": [
    "syntax-async-functions",
    "transform-regenerator"
  ],
  "ignore": [
    "dist",
    "node_modules"
  ],
  "env": {
    "development": {
      "plugins": [["babel-plugin-module-alias", [
        {"src": "./src", "expose": "~"}
      ]]]
    },
    "production": {
      "plugins": [["babel-plugin-module-alias", [
        {"src": "./lib", "expose": "~"}
      ]]]
    }
  }
}
